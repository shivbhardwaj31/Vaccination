<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.vaccicare.controller.RegisterController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.vaccicare.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>VacciScheduler</servlet-name>
	<servlet-class>com.vaccicare.controller.VacciSchedulerController</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>VacciScheduler</servlet-name>
	<url-pattern>/vaccischeduler</url-pattern>
	</servlet-mapping>
	
	
		<servlet>
		<servlet-name>vaccireminder</servlet-name>
		<servlet-class>com.vaccicare.controller.VacciReminderController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>vaccireminder</servlet-name>
		<url-pattern>/vaccireminder</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.vaccicare.controller.LogoutController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
